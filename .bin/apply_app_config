#!/bin/bash

cd `dirname "$0"`

# Apply app config to all components
cd ../app_config
./node_modules/.bin/esbuild --platform=node --bundle apply.ts | node

# electron/package.json relies on app_config but is static, so must also be updated
cd ../electron
./node_modules/.bin/esbuild --platform=node --bundle update_package_json.ts | node
