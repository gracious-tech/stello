#!/bin/bash
# Setup project for development (should be re-run safe)
set -e
cd "$(dirname "$(dirname "$0")")"


# Install packages
pipenv sync
npm ci
cd app
npm ci
cd ../app_config
npm ci
cd ../displayer
npm ci
cd ../electron
npm ci
cd ..


# Load reactions
.bin/reactions


